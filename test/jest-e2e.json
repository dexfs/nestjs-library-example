{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@dexfs/logging-interceptor/(.*)": "<rootDir>/../libs/logging-interceptor/src/$1",
    "@dexfs/logging-interceptor": "<rootDir>/../libs/logging-interceptor/src"
  }
}